node_modules/
dist/
out/
build/
.cache/
coverage/
.next/
.nuxt/
.svelte-kit/
.turbo/
.parcel-cache/
*.log
npm-debug.log*
yarn-debug.log*
yarn-error.log*
pnpm-debug.log*

# Python
__pycache__/
*.py[cod]
*.pyo
*.pyd
.venv/
venv/
.env
.env.*
.mypy_cache/
.pytest_cache/
.coverage
coverage.xml
*.egg-info/
